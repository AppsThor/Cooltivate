<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="CircleProgressView">
        <attr name="circleProgressThickness" format="dimension" />
        <attr name="circleProgressColor" format="color" />
        <attr name="circleProgressValue" format="integer" />
        <attr name="circleProgressStartAngle" format="integer" />
        <attr name="circleProgressText" format="string" />
        <attr name="circleProgressTextColor" format="color" />
        <attr name="circleProgressTextSize" format="dimension" />
        <attr name="circleProgressStartAnimation" format="enum">
            <enum name="none" value="0" />
            <enum name="roll" value="1" />
            <enum name="fadeIn" value="2" />
            <enum name="incremental" value="3" />
            <enum name="thicknessExpand" value="4" />
        </attr>
        <attr name="circleProgressStartAnimationDuration" format="integer" />
    </declare-styleable>
</resources>
